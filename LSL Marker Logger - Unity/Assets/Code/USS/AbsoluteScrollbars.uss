.absolute-scrollbar-container {
}

.absolute-scrollbar-container .unity-scroller--vertical {
    position: absolute;
    right: 0;
    height: 100%;
}

.control-button.button-toggle-active {
    background-color: rgb(79, 79, 79);
    padding-left: 5px;
    padding-right: 5px;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-bottom: 0;
    height: 100%;
    min-height: 100%;
    max-height: 100%;
}
